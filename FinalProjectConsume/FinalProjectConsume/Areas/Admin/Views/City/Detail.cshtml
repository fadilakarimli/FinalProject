@model FinalProjectConsume.Models.City.City

@{
    ViewData["Title"] = "City Detail";
}

<div class="container" style="margin-left:300px; margin-top:50px;">
    <h2>@ViewData["Title"]</h2>

    <div class="form-group">
        <label>Name:</label>
        <p>@Model.Name</p>
    </div>

    <div class="form-group">
        <label>Country:</label>
        <p>@Model.CountryName</p>
    </div>

    <p>
        <a asp-action="Index" class="btn btn-secondary">Back</a>
    </p>
</div>
