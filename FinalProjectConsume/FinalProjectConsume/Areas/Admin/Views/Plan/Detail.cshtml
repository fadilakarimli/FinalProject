@model FinalProjectConsume.Models.Plan.Plan

<div class="container mt-5" style="margin-left:300px;">
    <h2>Plan Detail</h2>
    <dl class="row">
        <dt class="col-sm-2">Day</dt>
        <dd class="col-sm-10">@Model.Day</dd>

        <dt class="col-sm-2">Title</dt>
        <dd class="col-sm-10">@Model.Title</dd>

        <dt class="col-sm-2">Description</dt>
        <dd class="col-sm-10">@Model.Description</dd>

        <dt class="col-sm-2">Tour Id</dt>
        <dd class="col-sm-10">@Model.TourId</dd>
    </dl>
    <a asp-action="Index" class="btn btn-secondary">Back to List</a>
</div>